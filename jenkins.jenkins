pipeline {
    agent any
	
    stages {
	stage('Clone'){
	    steps{
		bat("""
		    git clone https://github.com/hw2cb/jeknkins.git
		""")
	    }
	}
        stage('Build') {
            steps {
                sh 'make' 
                bat 'start cmd.exe /c C:\\Repository\\jeknkins\\build.bat'
            }
        }
    }
}